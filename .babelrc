{
    "plugins": [
        "syntax-dynamic-import"
    ],
    "presets": [
        [
            "@babel/preset-react",
            {
                "pragma": "h",
                "pragmaFrag": "Fragment"
            }
        ],
        [
            "@babel/preset-env",
            {
                "modules": false,
                "targets": [
                    "last 1 version",
                    "> 1%",
                    "maintained node versions",
                    "not dead"
                ]
            }
        ]
    ]
}